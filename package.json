{
  "name": "sastronegoro-blog",
  "version": "1.0.0",
  "description": "Blog Personal R. Ilham Sastronegoro",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/radenpioneer/blog.git"
  },
  "scripts": {
    "gulp:build": "gulp build",
    "gulp:prod": "gulp --silent build",
    "gulp:watch": "gulp watch",
    "del:dist": "npx del-cli dist",
    "del:cache": "npx del-cli .cache",
    "eleventy:build": "npx cross-env ELEVENTY_ENV=dev npx @11ty/eleventy",
    "eleventy:prod": "npx cross-env ELEVENTY_ENV=prod npx @11ty/eleventy --quiet",
    "eleventy:serve": "npx cross-env ELEVENTY_ENV=dev npx @11ty/eleventy --serve",
    "eleventy:debug": "npx cross-env ELEVENTY_ENV=dev DEBUG=* npx @11ty/eleventy",
    "build": "npm-run-all -s gulp:build eleventy:build",
    "build:prod": "npm-run-all -s gulp:prod eleventy:prod",
    "serve": "npm-run-all -s gulp:build -p gulp:watch eleventy:serve",
    "debug": "npm-run-all -s gulp:build eleventy:debug",
    "clean": "npm-run-all -s del:dist del:cache",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "start": "npm run serve"
  },
  "keywords": [],
  "author": "R. Ilham Sastronegoro",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@ampproject/eleventy-plugin-amp": "^0.5.2",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eleventy-plugin-pwa": "^1.0.8",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "moment": "^2.27.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "dark-mode-toggle": "^0.7.1",
    "html-minifier": "^4.0.0",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.10.0"
  }
}
